{
    "name": "@collections/all",
    "version": "0.0.1",
    "license": "MIT",
    "exports": "./mod.ts",
    "tasks": {
        "setup": "deno run -A setup.ts",
        "dev": "deno run --watch main.ts",
        "test": "deno test --coverage=./coverage"
    },
    "workspace": ["./algorithm", "./core", "./graph", "./linked-list", "./queue", "./stack", "./tree"],
    "test": {
        "exclude": ["**/test-suite.test.ts"]
    },
    "imports": {
        "@algorithm/": "./algorithm/",
        "@graph/": "./graph/",
        "@linked-list/": "./linked-list/",
        "@queue/": "./queue/",
        "@core/": "./core/",
        "@stack/": "./stack/",
        "@tree/": "./tree/",
        "@std/assert": "jsr:@std/assert@1",
        "fast-check": "npm:fast-check@^4.2.0"
    },
    "fmt": {
        "lineWidth": 180,
        "semiColons": true,
        "singleQuote": true,
        "indentWidth": 4
    }
}
